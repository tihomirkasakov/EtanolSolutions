<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.example.etanolsolutions">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher_rakia"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:roundIcon="@mipmap/ic_launcher_rakia_round"
            android:theme="@style/AppTheme" tools:ignore="AllowBackup">
        <activity android:name=".SolutionActivity" android:screenOrientation="portrait"
                  tools:ignore="LockedOrientationActivity">
        </activity>
        <activity android:name=".CorrectionActivity" android:screenOrientation="portrait"
                  tools:ignore="LockedOrientationActivity">
        </activity>
        <activity android:name=".MainActivity" android:screenOrientation="portrait"
                  tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>